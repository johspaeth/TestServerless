  "scripts": {
    "start": "PORT=3001 react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.+(ts|tsx|js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint",
    "prepare": "cd .. && husky install dashboard-app/.husky",
    "playwright:local": "NODE_ENV=localdev jest",
    "cy:open": "NODE_ENV=localdev cypress open",
    "cy:run": "NODE_ENV=localdev cypress run",
    "playwright:ci": "jest",
    "cy:ci": "cypress run",
    "test:local": "npm run playwright:local && npm run cy:run",
    "test:ci": "npm run cy:ci",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
